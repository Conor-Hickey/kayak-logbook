{
	"$schema": "https://biomejs.dev/schemas/1.5.0/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"nursery": {
				"noUnusedImports": "warn"
			},
			"correctness": {
				"noUnusedVariables": "warn"
			}
		}
	},
	"files": {
		"ignoreUnknown": true
	}
}